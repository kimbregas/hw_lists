<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="helloworld.example.com.simple_homework_list">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".hw_lists">
            <intent-filter>
                <action android:name="helloworld.example.com.simple_homework_list" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".desc_calcu" />
        <activity android:name=".desc_timetable" />
        <activity android:name=".desc_tipcalcu" />
        <activity android:name=".time_table" />
        <activity android:name=".tip_calculator" />
        <activity android:name=".mini_calculator" />
        <activity android:name=".change_password"></activity>
    </application>

</manifest>
